package esempio3;

public abstract class Triangolo extends Forma {
	private double lato1, lato2, lato3;

	public Triangolo(double lato1, double lato2, double lato3) {
		if(lato1<lato2+lato3 || lato2<lato1+lato3 || lato3 < lato1+lato2)
		this.lato1 = lato1;
		this.lato2 = lato2;
		this.lato3 = lato3;
	}

	@Override
	public double area() {
		// TODO Auto-generated method stub
		return 0;
	}

	@Override
	public double perimetro() {
		// TODO Auto-generated method stub
		return 0;
	}

	@Override
	public String nome() {
		return "Triangolo qualsiasi";
	}

}
