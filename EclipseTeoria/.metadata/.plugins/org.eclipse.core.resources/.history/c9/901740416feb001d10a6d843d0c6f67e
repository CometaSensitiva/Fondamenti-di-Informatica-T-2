package esempio9;

import java.text.Normalizer;
import java.util.Comparator;

public class PersonaNomeAlphabeticalComparator implements Comparator<Persona>{
	@Override
	public int compare(Persona p1, Persona p2) {
		String s1 = Normalizer.normalize(p1.nome().toLowerCase(), Normalizer.Form.NFD);
		String s2 = Normalizer.normalize(p2.nome().toLowerCase(), Normalizer.Form.NFD);
		return s1.compareTo(s2);
	}
}
