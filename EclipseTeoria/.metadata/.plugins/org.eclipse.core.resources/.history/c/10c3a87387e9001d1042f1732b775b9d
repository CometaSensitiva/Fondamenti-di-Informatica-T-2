package esempio3;

public class ImplIsoscelesTriangle extends ImplTriangle implements IsoscelesTriangle {

	public ImplIsoscelesTriangle(double base, double sides) {
		super(base, sides, sides);
	}

	@Override
	public double getBase() {
		return getSide1();
	}

	@Override
	public double getSide() {
		return getSide2();
	}

	@Override
	public String getName() {
		return "A Triangle Isosceles";
	}

	public String toString() {
		return getName() + "[side1=" + getSide1() + ", side2=" + getSide2() + ", side3=" + getSide3()
		+ ", angleOppositeSide1=" + getAngleOppositeSide1() + ", angleOppositeSide2=" + getAngleOppositeSide2()
		+ ", angleOppositeSide3=" + getAngleOppositeSide3() + ", area" + getArea() + ",perimeter"
		+ getPerimeter() + "]";
	}

}
