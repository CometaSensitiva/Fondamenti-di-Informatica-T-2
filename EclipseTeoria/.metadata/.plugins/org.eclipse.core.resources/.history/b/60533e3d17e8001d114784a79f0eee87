package esempio2;

/*This abstract class represents an aquatic animal that extends the Animal class.
*/
public abstract class AnimaleAcquatico extends Animale {

	protected AnimaleAcquatico(String s) {
		super(s);
	}

	@Override
	public String vive() {
		return "nell'acqua";
	}

	@Override
	public String nome() {
		return "un animale acquatico";
	}

}
