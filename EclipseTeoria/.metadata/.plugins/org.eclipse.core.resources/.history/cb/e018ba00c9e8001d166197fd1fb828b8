package esempio3;

public interface Triangle extends Shape{
	
	
	public double height1();
	
	public double height2();
	
	public double height3();
	
	public double angleOppositeSide1();
	
	public double angleOppositeSide2();
	
	public double angleOppositeSide3();
	
	public double side1();
	
	public double side2();
	
	public double side3();
}
